   <SplitMenuButton layoutX="651.0" layoutY="474.0" mnemonicParsing="false" text="SplitMenuButton">
     <items>
       <MenuItem mnemonicParsing="false" text="Action 1" />
       <MenuItem mnemonicParsing="false" text="Action 2" />
     </items>
   </SplitMenuButton>
   <Label fx:id="choiceBoxLabel" layoutX="227.0" layoutY="575.0" prefHeight="55.0" prefWidth="330.0" text="From combobox">
      <font>
         <Font name="System Bold" size="30.0" />
      </font>
   </Label>
   <Slider fx:id="slider" layoutX="877.0" layoutY="115.0" majorTickUnit="10.0" orientation="VERTICAL" prefHeight="325.0" prefWidth="16.0" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="10.0" />
   <Label fx:id="sliderLabel" layoutX="227.0" layoutY="662.0" prefHeight="55.0" prefWidth="330.0" text="From slider">
      <font>
         <Font name="System Bold" size="30.0" />
      </font>
   </Label>
   <ProgressBar layoutX="705.0" layoutY="583.0" prefWidth="200.0" />
   <ProgressBar fx:id="progressBar" layoutX="705.0" layoutY="630.0" prefWidth="200.0" progress="0.0" />
   <Button layoutX="705.0" layoutY="666.0" mnemonicParsing="false" onAction="#onProgressBarIncrement" prefHeight="24.0" prefWidth="83.0" text="inc" />
   <Label fx:id="progressBarLabel" layoutX="805.0" layoutY="666.0" prefHeight="24.0" prefWidth="154.0" text="value of progress" />
   <ListView fx:id="listView" layoutX="1012.0" layoutY="247.0" prefHeight="200.0" prefWidth="200.0" />
   <Label fx:id="listViewLabel" alignment="CENTER_RIGHT" layoutX="1012.0" layoutY="193.0" prefHeight="24.0" prefWidth="200.0" text="List view items">
      <font>
         <Font size="16.0" />
      </font>
   </Label>
   <TreeView fx:id="treeView" layoutX="1019.0" layoutY="493.0" onMouseClicked="#selectTreeItem" prefHeight="311.0" prefWidth="303.0" />
    <MenuBar prefHeight="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#onMenuBarItemClick" text="Menu item with shortcut">
                <accelerator>
                   <KeyCodeCombination alt="DOWN" code="N" control="UP" meta="UP" shift="UP" shortcut="UP" />
                </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Close1" />
            <MenuItem mnemonicParsing="false" text="Close2">
                <accelerator>
                    <KeyCodeCombination alt="DOWN" code="X" control="UP" meta="UP" shift="UP" shortcut="UP" />
                </accelerator>
            </MenuItem>
            <MenuItem mnemonicParsing="false" text="Close3" />
            <MenuItem mnemonicParsing="false" text="Close4" />
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Delete" />
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About" />
        </Menu>
    </MenuBar>
</AnchorPane>
